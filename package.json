{
  "name": "@yayaadev/sermo-models",
  "version": "1.0.1",
  "description": "TypeScript models for Sermo API (HTTP and WebSocket) generated from OpenAPI specifications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "gradle generateTypeScript generateWebSocketTypeScript createCombinedIndexFile && npm run compile",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist/ build/generated/typescript/ build/generated/typescript-websocket/"
  },
  "keywords": [
    "sermo",
    "api",
    "models",
    "typescript",
    "openapi",
    "speech",
    "language-learning",
    "websocket",
    "real-time"
  ],
  "author": "Sermo Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sermo-models.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}